@article{Moro2019,
  title = {Loss of {{Morphology}} in {{Alorese}} ({{Austronesian}}): {{Simplification}} in {{Adult Language Contact}}},
  shorttitle = {Loss of {{Morphology}} in {{Alorese}} ({{Austronesian}})},
  author = {Moro, Francesca R.},
  year = {2019},
  month = aug,
  journal = {Journal of Language Contact},
  volume = {12},
  number = {2},
  pages = {378--403},
  issn = {1877-4091, 1955-2629},
  doi = {10.1163/19552629-01202005},
  abstract = {This paper discusses historical and ongoing morphological simplification in Alorese, an Austronesian language spoken in eastern Indonesia. From comparative evidence, it is clear that Alorese lost almost all of its morphology over several hundred years as a consequence of language contact (Klamer, 2012, to appear). By providing both linguistic and cultural-historical evidence, this paper shows that Alorese has historically undergone morphological simplification as a result of second language (L2) learning. The first part of the paper presents a case study comparing the use of subject agreement prefixes in Alorese L1 speakers (n=6) and Alorese L2 speakers (n=12). The results show that L2 speakers deviate from the native norm, and tend to use one prefix as default agreement. The variation found among L2 speakers reveals an ongoing change possibly leading to the restructuring of the Alorese agreement system. The second part of the paper applies models of linguistic change (Kusters, 2003; Trudgill, 2011) to the Alorese community and shows that Alorese has been, and still is, spoken in a community with a large number of L2 speakers, where morphological simplification is expected to occur.},
  file = {/Users/eri/Zotero/storage/IUZCLMPG/Moro - 2019 - Loss of Morphology in Alorese (Austronesian) Simp.pdf}
}

