@book{Harrison1990,
  title = {Stealing {{People}}'s {{Names}}: {{History}} and {{Politics}} in a {{Sepik River Cosmology}}},
  shorttitle = {Stealing {{People}}'s {{Names}}},
  author = {Harrison, Simon J.},
  year = {1990},
